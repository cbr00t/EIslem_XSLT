  <xsl:param name="_kodYerineSiraNomu" select="$xroot/cac:AdditionalDocumentReference/cbc:ID [parent::node()/cbc:DocumentType = 'PARAM' and parent::node()/cbc:DocumentTypeCode = 'KOD_YERINE_SIRANO']"/>
